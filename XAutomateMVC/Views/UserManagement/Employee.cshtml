<script>
    $(document).ready(function ($) {
        debugger;
        var dropdown = document.getElementsByClassName("dropdown-btn");
        var i;

        for (i = 0; i < dropdown.length; i++) {
            dropdown[i].addEventListener("click", function () {
                this.classList.toggle("active");
                var dropdownContent = this.nextElementSibling;
                if (dropdownContent.style.display === "block") {
                    dropdownContent.style.display = "none";
                } else {
                    dropdownContent.style.display = "block";
                }
            });
        }
        var screens = sessionStorage.getItem("Screen");
        if (screens == null || screens == "" || screens == undefined) {
            window.location.href = "/Home/Index";
        }
        var RoleName = sessionStorage.getItem("RoleName");
        var Screensplit = screens.split("|");
        for (var i = 0; i < Screensplit.length; i++) {
            var val = Screensplit[i];
            var show = val.split(",");
            var val2 = show["0"];
            var val1 = show["1"];
            switch (val2) {
                case "Connection":
                    if (val1 == "Show") {
                        $("#Menuconnection").removeClass("hidden");
                    }
                    else {
                        $("#Menuconnection").addClass("hidden");
                    }
                    break;
                case "Suite":
                    if (val1 == "Show") {
                        $("#MenuSuites").removeClass("hidden");
                    }
                    else {
                        $("#MenuSuites").addClass("hidden");
                    }
                    break;
                case "Approach":
                    if (val1 == "Show") {
                        $("#MenuTestApproach").removeClass("hidden");
                    }
                    else {
                        $("#MenuTestApproach").addClass("hidden");
                    }
                    break;
                case "Testcases":
                    if (val1 == "Show") {
                        $("#MenuTestcases").removeClass("hidden");
                    }
                    else {
                        $("#MenuTestcases").addClass("hidden");
                    }
                    break;
                case "etldb":
                    if (val1 == "Show") {
                        $("#MenuEtlrules").removeClass("hidden");
                    }
                    else {
                        $("#MenuEtlrules").addClass("hidden");
                    }
                    break;
                case "Release":
                    if (val1 == "Show") {
                        $("#MenuRelease").removeClass("hidden");
                    }
                    else {
                        $("#MenuRelease").addClass("hidden");
                    }
                    break;
                case "Executions":
                    if (val1 == "Show") {
                        $("#MenuExecution").removeClass("hidden");
                    }
                    else {
                        $("#MenuExecution").addClass("hidden");
                    }
                    break;
                case "TestReport":
                    if (val1 == "Show") {
                        $("#MenuTestReport").removeClass("hidden");
                    }
                    else {
                        $("#MenuTestReport").addClass("hidden");
                    }
                    break;
                case "Analytics":
                    if (val1 == "Show") {
                        $("#MenuAnaltics").removeClass("hidden");
                    }
                    else {
                        $("#MenuAnaltics").addClass("hidden");
                    }
                    break;
                case "Usermanage":
                    if (val1 == "Show") {
                        $("#MenuUserMangment").removeClass("hidden");
                    }
                    else {
                        $("#MenuUserMangment").addClass("hidden");
                    }
                    break;
            }

        }
        $("#Status").prop('checked', true);
        $("#update").addClass("hidden");
        $(document).on("click", "#Save", function () {
            debugger;
            $("#Success").html("");
            $("#Error").html("");
            var Active = 1;
            var EmployeeNo = $("#Employee_No").val();
            if (EmployeeNo == null || EmployeeNo == "" || EmployeeNo == undefined) {
                $("#Error").html("Please Enter Employee Name");
                return false;
            }
            var Name = $("#Employee_Name").val();
            if (Name == null || Name == "" || Name == undefined) {
                $("#Error").html("Please Enter Employee Name");
                return false;
            }
            var E_Password = $("#password").val();
            if (E_Password == null || E_Password == "" || E_Password == undefined) {
                $("#Error").html("Please Enter Password");
                return false;
            }
            var E_Role = $("#ProductId").val();
            if (E_Role == null || E_Role == "" || E_Role == undefined) {
                $("#Error").html("Please Enter Employee Role");
                return false;
            }

            var des = $("#Description").val();
            if ($('#Status').is(':checked')) {
                Active = 1;
            }
            else {
                $("#Error").html("Please Check Active Check box");
            }
            var Email = $("#Employee_emailId").val();
            if (Email == null || Email == "" || Email == undefined) {
                $("#Error").html("Please Enter Email");
                return false;
            }

            $.ajax(
                {
                    type: "GET", //HTTP POST Method
                    url: "/UserManagement/SaveEmployee", // Controller/View
                    datatype: "json",
                    data: {
                        Emp_No: EmployeeNo,
                        E_Name: Name,
                        Emp_pass: E_Password,
                        Emp_role: E_Role,
                        Status: Active,
                        EmailId: Email,
                        Description: des,
                    },
                    headers: {
                        "Content-Type": JSON,
                        'Authorization': localStorage.getItem("AuthoToken"),
                    },
                    success: function (data) {
                        debugger;
                        if (data == "Auth Fail") {
                            $("#Error").html("Authentication Failed,Your Session Failed");
                            return false;
                        }
                        if (data == "Success") {
                            $("#Success").html("Employee Successfully Saved");
                            Bindrid(1);
                            clear();
                        }
                        else {
                            $("#Error").html("Already Exits Employee");
                        }

                    },
                    error: function (Result) {

                    }

                });
        });

        $(document).on("click", "#update", function () {
            debugger;
            $("#Success").html("");
            $("#Error").html("");
            var LoginId = $("#LoginId").val();
            var Active = 1;
            var EmployeeNo = $("#Employee_No").val();
            if (EmployeeNo == null || EmployeeNo == "" || EmployeeNo == undefined) {
                $("#Error").html("Please Enter Employee Name");
                return false;
            }
            var Name = $("#Employee_Name").val();
            if (Name == null || Name == "" || Name == undefined) {
                $("#Error").html("Please Enter Employee Name");
                return false;
            }
            var E_Password = $("#password").val();
            if (E_Password == null || E_Password == "" || E_Password == undefined) {
                $("#Error").html("Please Enter Password");
                return false;
            }
            var E_Role = $("#ProductId").val();
            if (E_Role == null || E_Role == "" || E_Role == undefined) {
                $("#Error").html("Please Enter Employee Role");
                return false;
            }

            var des = $("#Description").val();
            if ($('#Status').is(':checked')) {
                Active = 1;
            }
            else {
                Active = 0;
            }
            var Email = $("#Employee_emailId").val();
            if (Email == null || Email == "" || Email == undefined) {
                $("#Error").html("Please Enter Email");
                return false;
            }
            $.ajax(
                {
                    type: "GET", //HTTP POST Method
                    url: "/UserManagement/UpdateEmployee", // Controller/View
                    datatype: "json",
                    data: {
                        Emp_No: EmployeeNo,
                        E_Name: Name,
                        Emp_pass: E_Password,
                        Emp_role: E_Role,
                        Status: Active,
                        Description: des,
                        EmailId: Email,
                        LoginId: LoginId,
                    },
                    headers: {
                        "Content-Type": JSON,
                        'Authorization': localStorage.getItem("AuthoToken"),
                    },
                    success: function (data) {
                        debugger;
                        if (data == "Auth Fail") {
                            $("#Error").html("Authentication Failed,Your Session Failed");
                            return false;
                        }
                        if (data == "Success") {
                            $("#Success").html("Employee Successfully Saved");
                            Bindrid(1);
                            clear();
                        }
                        else {
                            $("#Error").html("Already Exits Employee");
                        }

                    },
                    error: function (Result) {

                    }

                });
        });

        function clear() {
            $("#update").addClass("hidden");
            $("#Save").removeClass("hidden");
            $("#LoginId").val("");
            $("#Employee_Name").val("");
            $("#Description").val("");
            $("#Employee_No").val("");
            $("#password").val("");
            $("#ProductId").val("");
            $("#Employee_emailId").val("");
        }

        Bindrid(1);
        function Bindrid(status) {
            $.ajax(
                {
                    type: "GET", //HTTP POST Method
                    url: "/UserManagement/BindEmployee", // Controller/View
                    datatype: "json",
                    data: { status: status },
                    headers: {
                        "Content-Type": JSON,
                        'Authorization': localStorage.getItem("AuthoToken"),
                    },
                    success: function (data) {
                        debugger;
                        if (data == "Auth Fail") {
                            $("#Error").html("Authentication Failed,Your Session Failed");
                            return false;
                        }
                        $('#tblAuthgrid').empty();
                        if (data.length > 0) {
                            for (var i = 0; i < data.length; i++) {
                                var tr;
                                tr = $('<tr/>');
                                tr.append("<td OnClick='display(" + data[i].loginId + ");'  style='    cursor: pointer;'>" + data[i].employeeNo + "</td>");
                                tr.append("<td OnClick='display(" + data[i].loginId + ");'  style='    cursor: pointer;'>" + data[i].employeeName + "</td>");
                                tr.append("<td OnClick='display(" + data[i].loginId + ");'  style='    cursor: pointer;'>" + data[i].emailId + "</td>");
                                tr.append("<td OnClick='display(" + data[i].loginId + ");'  style='    cursor: pointer;'>" + data[i].description + "</td>");
                                tr.append("<td OnClick='display(" + data[i].loginId + ");'  style='    cursor: pointer;'>" + data[i].status + "</td>");
                                $('#tblAuthgrid').append(tr);
                            }

                        }

                    },
                    error: function (Result) {

                    }

                });
        }
    });
    function display(Login) {
        $("#Success").html("");
        $("#Error").html("");
        $("#update").removeClass("hidden");
        $("#Save").addClass("hidden");
        $.ajax(
            {
                type: "GET", //HTTP POST Method
                url: "/UserManagement/EditEmployee", // Controller/View
                datatype: "json",
                data: {
                    LoginId: Login,
                },
                headers: {
                    "Content-Type": JSON,
                    'Authorization': localStorage.getItem("AuthoToken"),
                },
                success: function (data) {
                    if (data == "Auth Fail") {
                        $("#Error").html("Authentication Failed,Your Session Failed");
                        return false;
                    }
                    debugger;
                    $("#LoginId").val(data["0"].loginId);
                    $("#Employee_Name").val(data["0"].employeeName);
                    $("#Description").val(data["0"].description);
                    $("#Employee_No").val(data["0"].employeeNo);
                    $("#password").val(data["0"].password);
                    $("#ProductId").val(data["0"].roleId);
                    $("#Employee_emailId").val(data["0"].emailId);
                    var status = data["0"].status;
                    if (status == "1") {
                        $("#Status").prop("checked", true);
                    }
                    else {
                        $("#Status").prop("checked", false);
                    }

                },
                error: function (Result) {

                }

            });
    }
   
</script>

