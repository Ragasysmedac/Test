    function display(roles) {
        $("#update").removeClass("hidden");
        $("#Save").addClass("hidden");
        $.ajax(
            {
                type: "GET", //HTTP POST Method
                url: "/UserManagement/EditRole", // Controller/View
                datatype: "json",
                data: {
                    RoleId: roles,
                },
                headers: {
                    "Content-Type": JSON,
                    'Authorization': localStorage.getItem("AuthoToken"),
                },
                success: function (data) {
                    debugger;
                    if (data == "Auth Fail") {
                        $("#Error").html("Authentication Failed,Your Session Failed");
                        return false;
                    }

                    $("#UserRoleId").val(data["0"].roleId);
                    $("#Role_Name").val(data["0"].roleName);
                    $("#Description").val(data["0"].description)
                    var screenname = data["0"].screenName;
                    var Screensplit = screenname.split("|");
                    for (var i = 0; i < Screensplit.length; i++) {
                        var val = Screensplit[i];
                        var show = val.split(",");
                        var val2 = show["0"];
                        var val1 = show["1"];
                        switch (val2) {
                            case "Connection":
                                $("input[name='connection'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Suite":
                                $("input[name='Suite'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Approach":
                                $("input[name='Approach'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Testcases":
                                $("input[name='Testcases'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "etldb":
                                $("input[name='etldb'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Release":
                                $("input[name='Release'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Executions":
                                $("input[name='Executions'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "TestReport":
                                $("input[name='Test_Report'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Analytics":
                                $("input[name='Analytics'][value=" + val1 + "]").prop("checked", true);
                                break;
                            case "Usermanage":
                                $("input[name='Usermanage'][value=" + val1 + "]").prop("checked", true);
                                break;
                        }

                    }
                    var status = data["0"].status;
                    if (status == "1") {
                        $("#Status").prop("checked", true);
                    }
                    else {
                        $("#Status").prop("checked", false);
                    }

                },
                error: function (Result) {

                }

            });
    }

</script>

