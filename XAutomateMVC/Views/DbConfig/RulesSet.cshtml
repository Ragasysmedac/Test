                                $("#Error").html("Authentication Failed,Your Session Failed");
                                return false;
                            }
                            var tr;
                            if (data.length > 0) {
                                for (var i = 0; i < data.length; i++) {
                                    tr = $('<tr/>');
                                    tr.append("<td OnClick='display(this," + data[i].rulesId + ");'  style='    cursor: pointer;'>" + data[i].suiteName + "</td>");
