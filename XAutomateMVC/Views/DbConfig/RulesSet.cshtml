                    $("#Error").html("Please Enter Rule Parameter");
                    return false;
                }
                //var DBparameter = $("#TestsuidId").val();
                //if (DBparameter == null || DBparameter == "" || DBparameter == undefined) {
                //    $("#Error").html("Please Select Connection");
                //    return false;
                //}
