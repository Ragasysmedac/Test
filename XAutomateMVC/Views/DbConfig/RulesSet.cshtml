                for (var i = 0; i < DemoName; i++) {
                    var values = $("#Ruleparameter" + i).val();
                    if (values == "" || values == "" || values == undefined) {
                        $("#Error").html("Please Enter Rule Parameter Name " + i);
                        return false;
                    }
                    if (i == 0) {
