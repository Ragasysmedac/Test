                            SuiteName: Rule,
                            RuleName: Name,
                            Parameter: DemoName,
                            rulecond: Rulecondition,
                            Status: Active,
                            Description: des,
                            Ruleparameter: rulesarray,
                          //  connection: DBparameter,
